  <!-- EN: slow-heal -->
  <WoundHealing_Slow.labelShortAdj>느린 회복</WoundHealing_Slow.labelShortAdj>
  <!-- EN: soft -->
  <WoundHealing_Slow.symbolPack.prefixSymbols.0.symbol>부드러운</WoundHealing_Slow.symbolPack.prefixSymbols.0.symbol>
  
