  <!-- EN: Trigger mutation -->
  <WarpedObelisk_Mutator.comps.CompObeliskTriggerInteractor.jobString>변이 작동 중</WarpedObelisk_Mutator.comps.CompObeliskTriggerInteractor.jobString>
